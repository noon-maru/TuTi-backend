{
  "name": "TuTi",
  "version": "1.0.0",
  "description": "TuTi backend",
  "main": "dist/app.js",
  "license": "MIT",
  "_moduleAliases": {
    "@routers": "dist/routers",
    "@controllers": "dist/controllers",
    "@models": "dist/models",
    "@middleware": "dist/middleware",
    "@config": "dist/config",
    "@public": "dist/public"
  },
  "scripts": {
    "start": "npx tsc -p . && node -r module-alias/register dist/app.js",
    "build": "npx tsc -p .",
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node -r tsconfig-paths/register\" src/app.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "mongoose": "^7.4.1",
    "multer": "^1.4.5-lts.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
